
<div class="jumbotron">
    <div class="row text-center">
        <div class="col-md-6">
            <h1>Chefs</h1>
        </div>

        <div class="col-md-6">
            <h1><a href="/Dishes">Dishes</a></h1>
        </div>
    </div>
</div>
<h2>Check out our roster of Chefs!</h2>
<a href="/New" role="button" class="btn btn-primary">Add a Chef</a>
<hr>
<div>
    <table class="table table-dark">
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th># of Dishes</th>
        </tr>
        <tbody>
            
                @foreach(var a in ViewBag.ChefData)
                {
                <tr>
                <td>@a.FirstName @a.LastName</td>
                <td>@a.DOB</td>
                <td>@a.CreatedDish.Count</td>
                 </tr>
                }
                
           
        </tbody>
    </table>
</div>