<div class="jumbotron">
    <div class="row text-center">
        <div class="col-md-6">
            <h1><a href="/">Chefs</a></h1>
        </div>

        <div class="col-md-6">
            <h1>Dishes</h1>
        </div>
    </div>
</div>
<h2>Yum! Take a look at our TASTY DISHES!</h2>
<a href="/NewDish" role="button" class="btn btn-primary">Add a Dish!</a>
<hr>
<div>
    <table class="table table-dark">
        <tr>
            <th>Name</th>
            <th>Chef</th>
            <th>Tastiness</th>
            <th>Calories</th>
        </tr>
        <tbody>
            
                @foreach(var j in ViewBag.DishData)
                {
                    <tr>
                        <td>@j.Name</td>
                        <td>@j.Creator.FirstName</td>
                        <td>@j.Tastiness</td>
                        <td>@j.Calories</td>
                    </tr>
                }
            
        </tbody>
    </table>
</div>